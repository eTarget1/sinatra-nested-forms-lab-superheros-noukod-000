<div class="team">
<h1>Team Name<%= @team.name%> </h1>
<h2>Team Motto: <%=@team.motto%></h2>
</div><br>

<h1>Heroes</h1>
<% @super_heroes.each do |hero| %>
  <div class="hero">
<p><strong>Hero Name: </strong><%= hero.name %></p>
<p><strong>Hero Power: </strong><%= hero.power %></p>
<p><strong>Hero Biography: </strong><%= hero.bio %></p>
</div><br>
<% end %>

